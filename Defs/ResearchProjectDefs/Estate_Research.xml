<Defs>

  <ResearchProjectDef>
    <defName>Estate_TechCandleLights</defName>
    <label>wax-powered lighting</label>
    <description>Allows you to create wax from meat at cooking stations and construct wax-fueled candelabras and chandeliers.</description>
    <baseCost>250</baseCost>
    <techLevel>Medieval</techLevel>
    <tags>
      <li>ClassicStart</li>
    </tags>
  </ResearchProjectDef>
  
  <ResearchProjectDef>
    <defName>Estate_TechWoodStoveFurnace</defName>
    <label>wood stove furnace</label>
    <description>Allows you to build wood-fueled stove furnaces to heat rooms and cook meals.</description>
    <baseCost>250</baseCost>
    <techLevel>Medieval</techLevel>
    <tags>
      <li>ClassicStart</li>
    </tags>
  </ResearchProjectDef>    
  
  <ResearchProjectDef>
    <defName>Estate_TechRefrigeratorBasic</defName>
    <label>electric refrigeration</label>
    <description>Allows you to build electric refrigerators to store meals.</description>
    <baseCost>400</baseCost>
    <techLevel>Industrial</techLevel>
    <prerequisites>
      <li>Electricity</li>
    </prerequisites>	
    <tags>
      <li>ClassicStart</li>
    </tags>
  </ResearchProjectDef>  


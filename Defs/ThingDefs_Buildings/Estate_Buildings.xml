<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  
  <ThingDef ParentName="ArtableFurnitureBase">
    <defName>Estate_Telescope</defName>
    <label>fancy telescope</label>
    <description>A fancy telescope for doing amateur astronomy. It's a relaxing and fun hobby for a certain kind of person.</description>
    <graphicData>
      <texPath>Things/Building/Joy/invernusTelescope</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawRotated>false</drawRotated>
      <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
      </shadowData>
    </graphicData>
    <fillPercent>0.15</fillPercent>
    <altitudeLayer>Building</altitudeLayer>
    <hasInteractionCell>true</hasInteractionCell>
    <interactionCellOffset>(0,0,1)</interactionCellOffset>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <minifiedDef>MinifiedThing</minifiedDef>
    <socialPropernessMatters>true</socialPropernessMatters>
    <building>
      <joyKind>Telescope</joyKind>
    </building>
    <costList>
      <Steel>50</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
	  <Gold>1</Gold>
    </costList>
    <statBases>
      <MaxHitPoints>75</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <MarketValue>400</MarketValue>
      <Mass>8.0</Mass>
      <JoyGainFactor>1</JoyGainFactor>
    </statBases>
	<comps>
		<li Class="CompProperties_RoomIdentifier">
			<roomStat>Impressiveness</roomStat>
		</li>
	</comps>
  </ThingDef>
  
  
  <ThingDef ParentName="TableGatherSpotBase">
    <defName>Estate_RoundTable</defName>
    <label>round table (1x1)</label>
    <graphicData>
      <texPath>Things/Building/Furniture/mrXuiryusRoundTable</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(2,2)</drawSize>
      <shadowData>
        <volume>(0.3, 0.3, 0.3)</volume>
      </shadowData>
    </graphicData>
      <castEdgeShadows>false</castEdgeShadows>
      <staticSunShadowHeight>0.0</staticSunShadowHeight>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <WorkToBuild>550</WorkToBuild>
      <Mass>3</Mass>
      <Flammability>1.0</Flammability>
      <Beauty>0.7</Beauty>
    </statBases>
    <size>(1,1)</size>
    <costStuffCount>22</costStuffCount>
  </ThingDef>

  <ThingDef ParentName="BuildingBase">
    <defName>WoodStoveFurnace</defName>
    <label>wood stove furnace</label>
    <thingClass>IndustrialAge.Objects.Building_WoodStove</thingClass>
    <description>Cooks meals and produces heat. Runs out of fuel after a few days. As with all heat sources, it must be placed indoors so it has a closed space to heat. Refuelable.</description>    
    <category>Building</category><thingCategories><li>Buildings</li></thingCategories>
    <graphicData>
      <texPath>Things/Building/Temperature/WoodStoveFurnace/Jecrell_WoodStoveFurnace</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <damageData>
        <rect>(0.2,0,0.6,0.1)</rect>
      </damageData>
	  <drawSize>(3,3)</drawSize>
      <shadowData>
        <volume>(0.25,0.4,0.7)</volume>
        <offset>(0,0,-0.1)</offset>
      </shadowData>	  
    </graphicData>
	<uiIconPath>UI/Icons/WoodStoveFurnace/WoodStoveFurnace_ico</uiIconPath>	
    <defaultPlacingRot>South</defaultPlacingRot>	
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <pathCost>50</pathCost>
    <constructEffect>ConstructMetal</constructEffect>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <fillPercent>0.25</fillPercent>
    <statBases>
      <Mass>130</Mass>		
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToBuild>2000</WorkToBuild>
      <Flammability>0.5</Flammability> 
    </statBases>
    <researchPrerequisites>
      <li>Estate_TechWoodStoveFurnace</li>
    </researchPrerequisites>	 	
    <selectable>true</selectable>
	<size>(1,1)</size>
    <costList>
      <Steel>70</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
	<minifiedDef>MinifiedThing</minifiedDef>	
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,1)</interactionCellOffset>	
    <soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
    <placeWorkers>
      <li>PlaceWorker_Heater</li>
    </placeWorkers>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
    <comps>
      <li Class="CompProperties_Flickable"/>	
      <li Class="CompProperties_Refuelable">
        <fuelConsumptionRate>10.0</fuelConsumptionRate>
        <fuelCapacity>20.0</fuelCapacity>
        <fuelConsumptionPerTickInRain>0.0006</fuelConsumptionPerTickInRain>
        <fuelFilter>
          <thingDefs>
            <li>WoodLog</li>
          </thingDefs>
        </fuelFilter>
        <destroyOnNoFuel>false</destroyOnNoFuel>
      </li>
      <li Class="CompProperties_Breakdownable"/>	  
      <li Class="CompProperties_Glower">
        <glowRadius>10</glowRadius>
        <glowColor>(252,187,113,0)</glowColor>
      </li>
      <li Class="CompProperties_HeatPusher">
	    <compClass>IndustrialAge.Objects.CompHeatPusherRefuelable</compClass>
        <heatPerSecond>21</heatPerSecond>
        <heatPushMaxTemperature>28</heatPushMaxTemperature>
      </li>
    </comps>
    <designationCategory>Temperature</designationCategory>
    <designationHotKey>Misc1</designationHotKey>
    <recipes>
      <li>CookMealSimple</li>
      <li>CookMealFine</li>
      <li>CookMealLavish</li>
      <li>Make_Pemmican</li>
      <li>BurnApparel</li>
      <li>BurnDrugs</li>
    </recipes>
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <isMealSource>true</isMealSource>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
  </ThingDef>
  

  <ThingDef ParentName="BuildingBase">
    <defName>Estate_Refrigerator</defName>
    <label>Refrigerator</label>
    <thingClass>IndustrialAge.Objects.Building_Refrigerator</thingClass>
    <graphicData>
      <texPath>Things/Building/Temperature/RefrigeratorBasic/Jecrell_Refrigerator</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <damageData>
        <rect>(0.05,0.1,0.9,0.9)</rect>
        <cornerTL>Damage/Corner</cornerTL>
        <cornerTR>Damage/Corner</cornerTR>		
      </damageData>
        <drawSize>(2.3,2.3)</drawSize>
      <shadowData>
        <volume>(0.25,0.4,0.7)</volume>
        <offset>(0,0,-0.1)</offset>
      </shadowData>		
    </graphicData>
    <rotatable>true</rotatable>	
	<uiIconPath>UI/Icons/Refrigerator/Refrigerator_ico</uiIconPath>
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.5</fillPercent>
    <pathCost>70</pathCost>
    <building>	
      <fixedStorageSettings>
        <priority>Important</priority>
        <filter>
          <thingDefs>
            <li>Beer</li>
			<li>MedicineHerbal</li>
          </thingDefs>
          <categories>
            <li>FoodMeals</li>
          </categories>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <priority>Important</priority>
        <filter>
          <thingDefs>
            <li>Beer</li>
			<li>MedicineHerbal</li>
          </thingDefs>		
          <categories>
            <li>FoodMeals</li>
          </categories>
        </filter>
      </defaultStorageSettings>
    </building>
    <researchPrerequisites>
      <li>Estate_TechRefrigeratorBasic</li>
    </researchPrerequisites>	
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
		<soundAmbientPowered>Refrigerator_Ambience</soundAmbientPowered>
        <basePowerConsumption>50</basePowerConsumption>
      </li>
      <li Class="CompProperties_Glower">
        <glowRadius>2</glowRadius>
        <glowColor>(255,214,170,0)</glowColor>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Breakdownable"/>
    </comps>	
    <inspectorTabs>
      <li>ITab_Storage</li>
    </inspectorTabs>
    <statBases>
      <Mass>80</Mass>		
      <WorkToBuild>450</WorkToBuild>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>0.2</Flammability>
    </statBases>
    <description>Meals stored in this will not deteriorate, even if outside.</description>
    <costList>
      <Steel>100</Steel>
	  <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
    <tickerType>Rare</tickerType>
    <neverMultiSelect>true</neverMultiSelect>
    <designationCategory>Temperature</designationCategory>
    <surfaceType>Item</surfaceType>
    <constructEffect>ConstructMetal</constructEffect>
	    <storedConceptLearnOpportunity>Jecrell_Refrigerator</storedConceptLearnOpportunity>			
  </ThingDef>
  
  
  
  <ThingDef ParentName="BuildingBase">
    <defName>Estate_Telegraph</defName>
    <label>telegraph</label>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
    <thingClass>Building_CommsConsole</thingClass>
    <graphicData>
      <texPath>Things/Building/Misc/Telegraph</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>	
      <drawSize>(2.5,1.5)</drawSize>
      <damageData>
        <rect>(0,0,3,1.8)</rect>
        <cornerBL>Damage/Corner</cornerBL>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
    <defaultPlacingRot>South</defaultPlacingRot>		
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <canOverlapZones>false</canOverlapZones>
    <rotatable>true</rotatable>
    <minifiedDef>MinifiedThing</minifiedDef>	
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <WorkToBuild>2200</WorkToBuild>
      <Flammability>1.0</Flammability>
      <Mass>30</Mass>	  
    </statBases>
    <description>The telegraph allows communication with remote traders as well as other factions for trade and diplomacy.</description>
    <size>(2,1)</size>
    <costList>
      <Steel>20</Steel>	  
      <ComponentIndustrial>3</ComponentIndustrial>
    </costList>
    <stuffCategories>
      <li>Woody</li>	  
    </stuffCategories>
    <costStuffCount>140</costStuffCount>	
    <interactionCellOffset>(0,0,1)</interactionCellOffset>
    <hasInteractionCell>true</hasInteractionCell>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <designationCategory>Misc</designationCategory>
		<building>
			<ai_chillDestination>false</ai_chillDestination>
		</building>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>200</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Breakdownable"/>
    </comps>
    <staticSunShadowHeight>0.5</staticSunShadowHeight>
    <designationHotKey>Misc3</designationHotKey>
    <researchPrerequisites>
      <li>Industrial_Telegraph</li>
    </researchPrerequisites>
  </ThingDef> 
  
  
</Defs>

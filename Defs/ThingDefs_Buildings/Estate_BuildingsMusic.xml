  <ThingDef ParentName="BuildingBase">
    <defName>Estate_Gramophone</defName>
	<thingClass>IndustrialAge.Objects.Building_Gramophone</thingClass>
    <label>gramophone</label>
    <description>An ancient windup device utilized to play sound recordings from bygone eras.</description>
    <category>Building</category><thingCategories><li>Buildings</li></thingCategories>
    <graphicData>
      <texPath>Things/Building/Misc/Gramophone/Jecrell_Gramophone</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
      <drawSize>(2.3,2.3)</drawSize>	  
      <damageData>
        <rect>(0.35,0.35,0.3,0.3)</rect>
      </damageData>
      <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
      </shadowData>	  	  	  
    </graphicData>
	<clearBuildingArea>false</clearBuildingArea>
    <altitudeLayer>MoteOverhead</altitudeLayer>
	<uiIconPath>Things/Building/Misc/Gramophone/Jecrell_Gramophone_ui</uiIconPath>
    <minifiedDef>MinifiedThing</minifiedDef>
    <socialPropernessMatters>true</socialPropernessMatters>
    <passability>PassThroughOnly</passability>
    <building>
	  <isEdifice>false</isEdifice>
	  <canBuildNonEdificesUnder>true</canBuildNonEdificesUnder>
      <joyKind>Gaming_Cerebral</joyKind>
    </building>
    <placeWorkers>
      <li>IndustrialAge.Objects.PlaceWorker_ListenArea</li>
    </placeWorkers>
    <comps>
      <li Class="CompProperties_Forbiddable" />
    </comps>	
	<pathCost>8</pathCost>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <fillPercent>0.15</fillPercent>
    <statBases>
      <Mass>5</Mass>	
      <MaxHitPoints>110</MaxHitPoints>
      <WorkToBuild>1500</WorkToBuild>
      <Flammability>1.0</Flammability>
	  <Beauty>10</Beauty>
      <MarketValue>500</MarketValue>
      <JoyGainFactor>1</JoyGainFactor>	  
    </statBases>
    <researchPrerequisites>
      <li>Estate_TechMusicLow</li>
    </researchPrerequisites>		
    <selectable>true</selectable>
    <costList>
      <Steel>50</Steel>
      <ComponentIndustrial>2</ComponentIndustrial>	
    </costList>
    <stuffCategories>
      <li>Metallic</li>
	  <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>60</costStuffCount>	
    <soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
	    <storedConceptLearnOpportunity>Jecrell_Gramophone</storedConceptLearnOpportunity>		
    <designationCategory>Joy</designationCategory>
	<modExtensions>
	  <li Class="JecsTools.BuildingExtension">
	    <wipeCategories>
		  <li>OnFurniture</li>
		</wipeCategories>
	  </li>
    </modExtensions>	
  </ThingDef>
  
  <ThingDef ParentName="BuildingBase">
    <defName>Estate_Radio</defName>
	<thingClass>IndustrialAge.Objects.Building_Radio</thingClass>
    <label>radio</label>
    <description>An ancient windup device utilized to play sound recordings from bygone eras. Now with autoplay.</description>
    <category>Building</category><thingCategories><li>Buildings</li></thingCategories>
    <graphicData>
      <texPath>Things/Building/Misc/Radio/Jecrell_Radio</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
      <drawSize>(2,2)</drawSize>	  
      <damageData>
        <rect>(0.35,0.35,0.3,0.3)</rect>
      </damageData>
      <shadowData>
        <volume>(0.3, 0.6, 0.3)</volume>
      </shadowData>	  
    </graphicData>
    <minifiedDef>MinifiedThing</minifiedDef>	
    <socialPropernessMatters>true</socialPropernessMatters>
	<uiIconPath>UI/Icons/Radio/Radio_ico</uiIconPath>
    <building>
      <joyKind>Gaming_Cerebral</joyKind>
	  <isEdifice>false</isEdifice>
	  <canBuildNonEdificesUnder>true</canBuildNonEdificesUnder>
    </building>
    <placeWorkers>
      <li>IndustrialAge.Objects.PlaceWorker_ListenArea</li>
    </placeWorkers>	
	<pathCost>8</pathCost>
    <tickerType>Normal</tickerType>
    <drawerType>RealtimeOnly</drawerType>
    <statBases>
      <Mass>3</Mass>
      <MaxHitPoints>110</MaxHitPoints>
      <WorkToBuild>1500</WorkToBuild>
      <Flammability>1.0</Flammability>
	  <Beauty>10</Beauty>
      <MarketValue>500</MarketValue>
      <JoyGainFactor>1</JoyGainFactor>	  
    </statBases>
    <researchPrerequisites>
      <li>Estate_TechMusicMed</li>
    </researchPrerequisites>
	<comps>
      <li Class="CompProperties_Forbiddable" />	  	
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <basePowerConsumption>20</basePowerConsumption>
      </li>
	</comps>
    <selectable>true</selectable>
    <costList>
      <Steel>50</Steel>
      <ComponentIndustrial>2</ComponentIndustrial>	
    </costList>
    <stuffCategories>
      <li>Metallic</li>
	  <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
	<clearBuildingArea>false</clearBuildingArea>
    <altitudeLayer>MoteOverhead</altitudeLayer>
    <passability>Standable</passability>
    <fillPercent>0.15</fillPercent>
    <costStuffCount>60</costStuffCount>	
    <soundImpactDefault>BulletImpact_Ground</soundImpactDefault>
    <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
    <resourcesFractionWhenDeconstructed>0</resourcesFractionWhenDeconstructed>
    <drawPlaceWorkersWhileSelected>true</drawPlaceWorkersWhileSelected>
	    <storedConceptLearnOpportunity>Jecrell_Gramophone</storedConceptLearnOpportunity>		
    <designationCategory>Joy</designationCategory>
	<modExtensions>
	  <li Class="JecsTools.BuildingExtension">
	    <wipeCategories>
		  <li>OnFurniture</li>
		</wipeCategories>
	  </li>
    </modExtensions>	
  </ThingDef>  
  
</Defs>
